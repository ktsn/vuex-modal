{
  "name": "vuex-modal",
  "version": "0.1.1",
  "author": "katashin",
  "description": "Simple modal using Vuex",
  "keywords": [
    "modal",
    "component",
    "Vue",
    "Vuex"
  ],
  "license": "MIT",
  "main": "dist/vuex-modal.cjs.js",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/ktsn/vuex-modal",
  "bugs": "https://github.com/ktsn/vuex-modal/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktsn/vuex-modal.git"
  },
  "scripts": {
    "prepublishOnly": "npm run release",
    "clean": "rm -rf dist",
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "build:example": "webpack --config examples/webpack.config.js",
    "dev": "chokidar \"src/**/*\" --initial --silent -c \"npm run build\"",
    "dev:example": "webpack-dev-server --inline --hot --config examples/webpack.config.js",
    "flow": "flow check",
    "lint": "eslint --fix \"@(src|scripts)/**/*.js\"",
    "release": "run-s lint flow clean build"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.5",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-power-assert": "^2.0.0",
    "chokidar-cli": "^1.2.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.0.1",
    "eslint-config-ktsn": "^1.0.3",
    "eslint-plugin-flowtype": "^2.49.3",
    "flow-bin": "^0.75.0",
    "npm-run-all": "^4.1.3",
    "power-assert": "^1.6.0",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "rollup-watch": "^4.3.1",
    "style-loader": "^0.21.0",
    "vue": "^2.5.16",
    "vue-loader": "^15.2.4",
    "vue-template-compiler": "^2.5.16",
    "vuex": "^3.0.1",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "peerDependencies": {
    "vue": "^2.0.0",
    "vuex": "^2.0.0"
  },
  "dependencies": {
    "vue-thin-modal": "^1.0.0"
  }
}
