{
  "name": "vuex-modal",
  "version": "0.0.1",
  "author": "katashin",
  "description": "Simple modal using Vuex",
  "keywords": [
    "modal",
    "component",
    "Vue",
    "Vuex"
  ],
  "license": "MIT",
  "main": "dist/vuex-modal.cjs.js",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/ktsn/vuex-modal",
  "bugs": "https://github.com/ktsn/vuex-modal/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktsn/vuex-modal.git"
  },
  "scripts": {
    "prepublish": "npm run release",
    "clean": "rm -rf dist",
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "build:example": "webpack --config examples/webpack.config.js",
    "dev": "chokidar \"src/**/*\" --initial --silent -c \"npm run build\"",
    "dev:example": "webpack-dev-server --inline --hot --config examples/webpack.config.js",
    "flow": "flow check",
    "lint": "eslint --fix \"@(src|scripts)/**/*.js\"",
    "release": "run-s lint flow clean build"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-power-assert": "^1.0.0",
    "chokidar-cli": "^1.2.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-config-ktsn": "^1.0.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "flow-bin": "^0.45.0",
    "npm-run-all": "^4.0.2",
    "power-assert": "^1.4.2",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.2",
    "rollup-watch": "^3.2.2",
    "style-loader": "^0.17.0",
    "vue": "^2.3.2",
    "vue-loader": "^12.0.3",
    "vue-template-compiler": "^2.3.2",
    "vuex": "^2.3.1",
    "webpack": "^2.5.0",
    "webpack-dev-server": "^2.4.5"
  },
  "peerDependencies": {
    "vue": "^2.0.0",
    "vuex": "^2.0.0"
  },
  "dependencies": {
    "vue-thin-modal": "^0.2.5"
  }
}
